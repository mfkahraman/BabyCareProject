@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_HomepageHeadComponent")

<body>

    @await Component.InvokeAsync("_HomepageTopBar")

    <!-- Hero Start -->
    @await Component.InvokeAsync("_HomepageHeroComponent")
    <!-- Hero End -->
    <!-- About Start -->
    @await Component.InvokeAsync("_HomepageAboutComponent")
    <!-- About End -->
    <!-- Service Start -->
    @await Component.InvokeAsync("_HomepageServiceComponent")
    <!-- Service End -->
    <!-- Programs Start -->
    @await Component.InvokeAsync("_HomepageProgramsComponent")
    <!-- Program End -->
    <!-- Events Start -->
    @await Component.InvokeAsync("_HomepageEventsComponent")
    <!-- Events End-->
    <!-- Team Start-->
    @await Component.InvokeAsync("_HomepageTeamComponent")
    <!-- Team End-->
    <!-- Testimonial Start -->
    @await Component.InvokeAsync("_HomepageTestimonialComponent")
    <!-- Testimonial End -->
    <!-- Footer Start -->
    @await Component.InvokeAsync("_HomepageFooterComponent")
    <!-- Footer End -->
    <!-- Back to Top -->
    <a href="#" class="btn btn-primary border-3 border-primary rounded-circle back-to-top"><i class="fa fa-arrow-up"></i></a>

    <!-- JavaScript Libraries -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/BabyCare-1.0.0/lib/wow/wow.min.js"></script>
    <script src="~/BabyCare-1.0.0/lib/easing/easing.min.js"></script>
    <script src="~/BabyCare-1.0.0/lib/waypoints/waypoints.min.js"></script>
    <script src="~/BabyCare-1.0.0/lib/lightbox/js/lightbox.min.js"></script>
    <script src="~/BabyCare-1.0.0/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/BabyCare-1.0.0/js/main.js"></script>
</body>

</html>